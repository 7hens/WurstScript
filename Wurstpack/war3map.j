// Trigger: Initialition
//===========================================================================
function Trig_Initialition_Func032Func001C takes nothing returns boolean
    if ( not ( GetPlayerSlotState(ConvertedPlayer(GetForLoopIndexA())) == PLAYER_SLOT_STATE_PLAYING ) ) then
        return false
    endif
    if ( not ( GetPlayerController(ConvertedPlayer(GetForLoopIndexA())) == MAP_CONTROL_USER ) ) then
        return false
    endif
    return true
endfunction

function Trig_Initialition_Actions takes nothing returns nothing
    call UseTimeOfDayBJ( false )
    call SetTimeOfDay( 4.00 )
    call CreateTextTagLocBJ( "TRIGSTR_165", GetRectCenter(gg_rct_Gebiet_110), 0, 10, 100, 0.00, 100, 0 )
    call CreateTextTagLocBJ( "TRIGSTR_131", GetRectCenter(gg_rct_Gebiet_103), 0, 10, 100, 100.00, 100.00, 0 )
    call AddWeatherEffectSaveLast( gg_rct_Gebiet_083, 'LRaa' )
    call EnableWeatherEffect( GetLastCreatedWeatherEffect(), true )
    call TriggerSleepAction( 0.10 )
    call AddWeatherEffectSaveLast( gg_rct_Gebiet_102, 'SNhs' )
    call AddWeatherEffectSaveLast( gg_rct_Gebiet_086, 'SNhs' )
    call EnableWeatherEffect( GetLastCreatedWeatherEffect(), true )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 12
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_Name_original[GetForLoopIndexA()] = GetPlayerName(ConvertedPlayer(GetForLoopIndexA()))
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    set udg_Name[1] = ( "|c00ff0303" + ( GetPlayerName(Player(0)) + "|r" ) )
    set udg_Name[2] = ( "|c000042ff" + ( GetPlayerName(Player(1)) + "|r" ) )
    set udg_Name[3] = ( "|c001ce6b9" + ( GetPlayerName(Player(2)) + "|r" ) )
    set udg_Name[4] = ( "|c00540081" + ( GetPlayerName(Player(3)) + "|r" ) )
    set udg_Name[5] = ( "|c00fffc01" + ( GetPlayerName(Player(4)) + "|r" ) )
    set udg_Name[6] = ( "|c00feba0e" + ( GetPlayerName(Player(5)) + "|r" ) )
    set udg_Name[7] = ( "|c0020c000" + ( GetPlayerName(Player(6)) + "|r" ) )
    set udg_Name[8] = ( "|c00e55bb0" + ( GetPlayerName(Player(7)) + "|r" ) )
    set udg_Name[9] = ( "|c00959697" + ( GetPlayerName(Player(8)) + "|r" ) )
    set udg_Name[10] = ( "|c007ebff1" + ( GetPlayerName(Player(9)) + "|r" ) )
    set udg_Name[11] = ( "|c00106246" + ( GetPlayerName(Player(10)) + "|r" ) )
    set udg_Name[12] = ( "|c004e2a04" + ( GetPlayerName(Player(11)) + "|r" ) )
    set udg_Name[13] = ( "|c004e2a04" + ( "Dummer Bürger" + "|r" ) )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 12
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set bj_forLoopBIndex = 1
        set bj_forLoopBIndexEnd = 12
        loop
            exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
            call SetPlayerAllianceStateBJ( ConvertedPlayer(GetForLoopIndexA()), ConvertedPlayer(GetForLoopIndexB()), bj_ALLIANCE_ALLIED_VISION )
            set bj_forLoopBIndex = bj_forLoopBIndex + 1
        endloop
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 12
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateFogModifierRectBJ( true, ConvertedPlayer(GetForLoopIndexA()), FOG_OF_WAR_VISIBLE, GetPlayableMapRect() )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    call AddSpecialEffectLocBJ( GetRectCenter(gg_rct_Teleport_effect), "Abilities\\Spells\\Human\\MassTeleport\\MassTeleportTo.mdl" )
    call CreateTextTagLocBJ( "TRIGSTR_038", OffsetLocation(GetRectCenter(gg_rct_Ausgang), -100.00, -80.00), 0, 10.00, 100, 100.00, 100.00, 0 )
    call CreateTextTagLocBJ( "TRIGSTR_062", OffsetLocation(GetRectCenter(gg_rct_TOR_1), -75.00, -100.00), 0, 9.00, 100, 0.00, 0.00, 0 )
    call CreateTextTagLocBJ( "TRIGSTR_063", OffsetLocation(GetRectCenter(gg_rct_TOR_2), -75.00, -100.00), 0, 9.00, 0.00, 100.00, 0.00, 0 )
    call CreateTextTagLocBJ( "TRIGSTR_064", OffsetLocation(GetRectCenter(gg_rct_TOR_3), -75.00, -100.00), 0, 9.00, 0.00, 0.00, 100.00, 0 )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 11
    loop
