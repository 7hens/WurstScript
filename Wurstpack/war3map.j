    call DisplayTimedTextToForce( GetPlayersAll(), 10.00, ( ( "Da wäre ich jetzt nicht hingegangen " + udg_Name[GetConvertedPlayerId(GetOwningPlayer(GetEnteringUnit()))] ) + "." ) )
